{
  "name": "text-file-processor",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@codegenie/serverless-express": "^4.16.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/swagger": "^11.2.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1692.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dynamoose": "^4.0.4",
    "nestjs-dynamoose": "^0.6.0",
    "nestjs-form-data": "^1.9.93",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "^3.810.0",
    "@aws-sdk/client-dynamodb": "^3.810.0",
    "@aws-sdk/client-iam": "^3.810.0",
    "@aws-sdk/client-lambda": "^3.810.0",
    "@aws-sdk/client-s3": "^3.810.0",
    "@aws-sdk/client-sts": "^3.810.0",
    "@aws-sdk/lib-dynamodb": "^3.810.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.1",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.24",
    "@testcontainers/localstack": "^10.25.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.18",
    "@types/supertest": "^6.0.3",
    "aws-cdk": "^2.1015.0",
    "axios": "^1.9.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "terser-webpack-plugin": "^5.3.14",
    "testcontainers": "^10.26.0",
    "ts-jest": "^29.3.3",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "webpack": "^5.99.8",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
